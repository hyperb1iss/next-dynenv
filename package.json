{
    "name": "next-dynenv",
    "version": "4.0.1",
    "description": "Dynamic runtime environment variables for Next.js - populate your environment at runtime, not build time.",
    "type": "module",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "import": "./build/index.js"
        }
    },
    "engines": {
        "node": ">=20.9.0"
    },
    "scripts": {
        "lint": "biome check . && prettier --check \"**/*.{md,mdx,yml,yaml}\"",
        "lint:fix": "biome check --write . && prettier --write \"**/*.{md,mdx,yml,yaml}\"",
        "lint:all": "biome check --write --unsafe . && prettier --write \"**/*.{md,mdx,yml,yaml}\"",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "test:integration": "node test-app/integration-test.mjs",
        "build": "tsc -p .",
        "build:watch": "pnpm build --watch",
        "audit-ci": "audit-ci",
        "semantic-release": "semantic-release"
    },
    "keywords": [
        "nextjs",
        "next.js",
        "runtime",
        "environment",
        "env",
        "dynamic",
        "runtime-env",
        "next-dynenv",
        "docker",
        "kubernetes",
        "twelve-factor"
    ],
    "author": "Stefanie Jane <info@hyperbliss.tech> (Forked from Expatfile.tax LLC)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/hyperb1iss/next-dynenv.git"
    },
    "bugs": {
        "url": "https://github.com/hyperb1iss/next-dynenv/issues"
    },
    "homepage": "https://github.com/hyperb1iss/next-dynenv#readme",
    "dependencies": {
        "react": "catalog:"
    },
    "devDependencies": {
        "@biomejs/biome": "catalog:",
        "@testing-library/jest-dom": "catalog:",
        "@testing-library/react": "catalog:",
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vitest/coverage-v8": "catalog:",
        "audit-ci": "catalog:",
        "jsdom": "catalog:",
        "next": "catalog:",
        "prettier": "catalog:",
        "react-dom": "catalog:",
        "semantic-release": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:"
    },
    "peerDependencies": {
        "next": "^15 || ^16",
        "react": "^19"
    },
    "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
    "pnpm": {
        "overrides": {
            "glob": ">=10.5.0",
            "brace-expansion": ">=2.0.2"
        }
    }
}
